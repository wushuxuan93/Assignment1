<h1>Contacts List</h1>
<div class="row">
    <div class="col">
        <table class="table table-striped table-bordered table-hover">
            <thread class="table-dark">
                <tr>
                    <th scope="col" class="text-center">#</th>
                    <th scope="col">Contact Name</th>
                    <th scope="col">Contact Number</th>
                    <th scope="col">Email Address</th>
                    <th></th>
                    <th></th>
                </tr>
            </thread>
            <tbody id="contactsList">
                <!--Repeatable Rows-->
                <% for(let count =0;count < contacts.length; count++) {%>
                <tr>
                    <th scope="row" class="text-center"><%= count + 1 %></th>
                    <td><%= contacts[count].Contact Name %></td>
                    <td><%= contacts[count].Contact Number %></td>
                    <td><%= contacts[count].Email Address %></td>
                    <td></td>
                    <td></td>
                </tr>
                <%} %>
            </tbody>
        </table>
    </div>

</div>